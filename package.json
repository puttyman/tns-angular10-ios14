    "preinstall": "npx npm-force-resolutions",
    "ngcc": "ngcc --properties es2017 module main --first-only",
    "ios": "ns run ios --bundle --no-hmr --env.aot",
    "build-ios": "ns build ios --bundle",
    "test-ios": "ns test ios --emulator",
    "build": "npm run clean && npm i && npm run recompile",
    "clean-generated": "rimraf \"./app/**/*.js\" && rimraf \"./app/**/*.css\" && tsc -p ./",
    "clean": "npx rimraf node_modules hooks platforms",
    "lint": "eslint 'app/**/*.ts'",
    "prettier": "prettier --config .prettierrc --write 'app/**/*.ts'",
    "recompile": "rimraf \"./app/**/*.css\" && rimraf \"./app/**/*.js\" && npm run ngcc && npm run rm-empty",
    "rm-empty": "find . -type d -empty -delete"
